<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.wasitec.sieveanalisis"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:icon="@drawable/icon_sieve_128_2"
        android:label="@string/app_name"
        android:theme="@style/TemaPersonalizadoMenu"
        android:hardwareAccelerated="true"
        android:allowBackup="false">
        <activity
            android:name="net.wasitec.sieveanalisis.InicioActivity"
            android:label="@string/app_name"
            android:configChanges="orientation|screenSize|screenLayout">

        </activity>
        <activity
            android:name="net.wasitec.sieveanalisis.Datos_Activity"
            android:label="@string/title_activity_principal" 
            android:icon="@drawable/ic_back"
            android:screenOrientation="landscape"
            android:configChanges="orientation|screenSize|screenLayout">
        </activity>
        <activity
            android:name="net.wasitec.sieveanalisis.Reporte_Activity"
            android:label="@string/title_activity_detalle" 
            android:icon="@drawable/ic_back"
            android:configChanges="orientation|screenSize|screenLayout">
        </activity>
        <activity
            android:name="net.wasitec.sieveanalisis.Malla_Activity"
            android:label="@string/title_activity_malla_" 
            android:icon="@drawable/ic_back"
            android:screenOrientation="landscape"
            android:configChanges="orientation|screenSize|screenLayout">
        </activity>
        
        <activity android:name="Splash_Activity">
            
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>	  
            
        </activity>
       
    </application>

</manifest>